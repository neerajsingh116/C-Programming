#include<stdio.h>
int main()
{
	int a,b,c;
	printf("Enter First Integer");
	scanf("%d",&a);
	printf("Enter Second Integer");
	scanf("%d",&b);
	printf("Enter Third Integer");
	scanf("%d",&c);
	
	if(a>b && a>c)
	printf("%d is Largest",a);
	if(b>a && b>c)
	printf("%d is Largest",b);
	else
	printf("%d is largest",c);
	
	return 0;
}
